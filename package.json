{
  "name": "reignarrwear",
  "version": "1.3.0",
  "author": "Mak Tarsis",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tsc:ssr": "tsc -p server/tsconfig.functions.json",
    "build:ssr": "ng run reignarr-wear:build:production && ng run reignarr-wear:server:production",
    "build:analyze:dev": "ng build --dev --stats-json && npx webpack-bundle-analyzer functions/browser/stats.json",
    "build:analyze:prod": "ng build --prod --stats-json && npx webpack-bundle-analyzer functions/browser/stats.json",
    "build:explorer:dev": "ng build && npx source-map-explorer functions/browser/vendor.js",
    "build:explorer:prod": "ng build --prod --source-map --output-hashing=none && npx source-map-explorer functions/browser/main.js",
    "firebase:deploy": "npm run build:ssr && npm run tsc:ssr && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.8.1",
    "@angular/animations": "6.1.7",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/fire": "^5.0.0",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "6.1.7",
    "@angular/http": "6.1.7",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@angular/platform-server": "6.1.7",
    "@angular/router": "6.1.7",
    "@firebase/app": "^0.3.3",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "core-js": "^2.5.4",
    "firebase": "^5.4.2",
    "firebase-admin": "6.0.0",
    "firebase-functions": "^2.0.5",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "ts-loader": "^5.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.8.1",
    "@angular-devkit/core": "0.8.1",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "6.1.7",
    "@angular/language-service": "6.1.7",
    "@types/googlemaps": "^3.30.8",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.1.4",
    "codelyzer": "~4.4.4",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.3",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "protractor": "^5.4.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-node-externals": "^1.7.2",
    "yarn": "^1.7.0"
  }
}
